{
  "name": "Heax",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Made by Anonymous",
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["src/background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["src/styles/styles.css"],
      "js": ["src/content.js", "src/starthack.js"]
    }
  ],
  "web_accessible_resources": ["src/assets/"],
  "permissions": ["tabs", "activeTab", "fontSettings"]
}
