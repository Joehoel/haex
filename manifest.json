{
  "name": "Timo",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Timo de vis",
  "options_page": "options.html",
  "browser_action": {  "default_popup": "popup.html"},
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["styles.css"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": ["Animations/*"]
}
